{
  "version": 2,
  "builds": [
    {
      "src": "rest/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "internalServer": true,
        "generateStaticRoutes": true,
        "serverFiles": ["rest/**", "_nuxt/content/**", "content/**"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/rest/index",
      "destination": "/rest/index.js"
    }
  ]
}